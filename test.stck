const FUNNY_NUMBER         69 end
const ANOTHER_FUNNY_NUMBER 42 end
const VERY_FUNNY_NUMBER    FUNNY_NUMBER 100 mul ANOTHER_FUNNY_NUMBER add end

const test 69 42 divmod swap drop end

macro print-if-funny
  dup FUNNY_NUMBER eq if
    "nice" puts drop
  else dup ANOTHER_FUNNY_NUMBER eq if*
    "nice" puts drop
  else dup VERY_FUNNY_NUMBER eq if*
    "very nice" puts drop
  else drop end
end

proc main
  69 print-if-funny
   print-if-funny
end
