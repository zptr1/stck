proc fib-recursion :: int -> int do
  dup 2 gt if
    dup  2 sub fib-recursion
    swap 1 sub fib-recursion
    add
  else
    drop 1
  end
end

proc fib-loop
  1 0 0 while dup 10 lt do
    1 add swap rot
    over add
    dup print
    rot
  end drop drop drop
end

proc main
  "fib(10) = " puts 10 fib-recursion print

  "Fibonacci sequence to 10th term:\n" puts
  fib-loop
end
